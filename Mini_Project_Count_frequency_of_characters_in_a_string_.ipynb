        "def frequency (string):\n",
        "  temp = { }\n",
        "  for i in string:\n",
        "    if i in temp:\n",
        "      temp[i] = temp[i] + 1\n",
        "    else:\n",
        "      temp[i] = 1\n",
        "  #print(\"Character Frequency:\", temp)\n",
        "  return temp\n",
        "\n",
        "string = input(\"Enter the string:\")\n",
        "frequency(string)"
